<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="800.0" spacing="20.0" style="-fx-background-color: #222;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.zespol.ui.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <HBox alignment="CENTER" prefHeight="40.0" prefWidth="760.0" spacing="20">
      <children>
         <HBox alignment="CENTER_LEFT" prefHeight="31.0" prefWidth="500.0">
            <children>
            
              <Label fx:id="welcomeText" prefHeight="17.0" prefWidth="125.0" text="Liczba pierwsza p:" textFill="WHITE" />
               <TextField prefHeight="25.0" prefWidth="230.0" />
               <Button alignment="CENTER" mnemonicParsing="false" text="Wygeneruj p">
                 <HBox.margin>
                   <Insets left="20" />
                 </HBox.margin>
               </Button>
            </children>
         </HBox>
         <HBox alignment="CENTER" prefHeight="31.0" prefWidth="300.0">
            <children>
               <Label prefHeight="17.0" prefWidth="113.0" text="Generator grupy g:" textFill="WHITE" />
               <TextField />
            </children>
         </HBox>
      </children>
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
   </HBox>
   <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="760.0">
      <children>
         <Label prefHeight="17.0" prefWidth="125.0" text="Klucz prywatny x: " textFill="WHITE" />
         <TextField prefHeight="25.0" prefWidth="235.0" />
         <Button mnemonicParsing="false" text="Wygeneruj x">
           <HBox.margin>
             <Insets left="20" />
           </HBox.margin>
         </Button>
         <Button mnemonicParsing="false" text="Zapisz">
             <HBox.margin>
                 <Insets left="20" />
             </HBox.margin>
         </Button>
         <Button mnemonicParsing="false" text="Wczytaj">
             <HBox.margin>
                 <Insets left="20" />
             </HBox.margin>
         </Button>
      </children>
   </HBox>
   <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="760.0">
      <children>
         <Label prefHeight="17.0" prefWidth="125.0" text="Y klucza publicznego: " textFill="WHITE" />
         <TextField prefHeight="25.0" prefWidth="235.0" />
         <Button mnemonicParsing="false" text="Wygeneruj Y">
           <HBox.margin>
             <Insets left="20" />
           </HBox.margin>
         </Button>
          <Button mnemonicParsing="false" text="Zapisz">
              <HBox.margin>
                  <Insets left="20" />
              </HBox.margin>
          </Button>
          <Button mnemonicParsing="false" text="Wczytaj">
              <HBox.margin>
                  <Insets left="20" />
              </HBox.margin>
          </Button>
      </children>
   </HBox>
</VBox>
